<!DOCTYPE html>
<html lang="en">
    <head>
        <% include head %>    
        <style type="text/css">
         #main-content{
         padding: 40px 20px 0px 20px; 
         margin: 40px 0px 0px 0px;
         border: none;
         }
      </style>    
    </head>

    <body>

        <header>
            <% include header %>
        </header>

        <div id="main-content">            
            <div class="panel panel-default">
                <ul class="list-group">
                     <% for(var i=0; i < result.length; i++) { %>
                        <li class="list-group-item">
                              <h4>
                                 <a href=<%= result[i].url %>>
                                    <%= result[i].name %>
                                 </a>
                              </h4>
                              <h5><%= result[i].snippet %></h5>                                                 
                        </li>                     
                     <% } %>                                    
                </ul>
            </div>
         
            
        </div>

        <footer>
            <% include footer %>            
        </footer>

    </body>
</html>